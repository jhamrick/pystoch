#!/usr/bin/python

import os
import unittest
import sys

from pytestgen_pystoch import PyUnitframework
from transform.test_transform import TestTransform

testlist = unittest.TestSuite()
testlist.addTest(unittest.makeSuite(PyUnitframework))
testlist.addTest(unittest.makeSuite(TestTransform))
result = unittest.TextTestRunner(verbosity=2).run(testlist)

if not result.wasSuccessful():
    sys.exit(1)
sys.exit(0)
